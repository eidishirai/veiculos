# Configuração do Firebase
# Crie um projeto em https://console.firebase.google.com/
# Vá em Project Settings > General > Your apps > Web app
# Copie as configurações para um arquivo chamado .env.local

NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=

# Configuração do Cloudinary (Upload de Imagens - GRATUITO)
# Crie uma conta em https://cloudinary.com/users/register/free
# Vá em Dashboard e copie os valores abaixo
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=
NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET=
